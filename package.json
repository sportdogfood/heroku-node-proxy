{
  "name": "cors-anywhere",
  "version": "0.4.4",
  "description": "CORS Anywhere is a reverse proxy which adds CORS headers to the proxied request. Request URL is taken from the path",
  "license": "MIT",
  "author": "Rob Wu <rob@robwu.nl>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rob--W/cors-anywhere.git"
  },
  "bugs": {
    "url": "https://github.com/Rob--W/cors-anywhere/issues/",
    "email": "rob@robwu.nl"
  },
  "keywords": [
    "cors",
    "cross-domain",
    "http-proxy",
    "proxy",
    "heroku"
  ],
  "main": "server.js",
  "files": [
    "lib/",
    "test/",
    "Procfile",
    "demo.html",
    "server.js"
  ],
  "scripts": {
    "start": "node server.js",
    "lint": "eslint .",
    "test": "mocha ./test/test*.js --reporter spec",
    "test-coverage": "istanbul cover ./node_modules/.bin/_mocha -- test/test.js test/test-ratelimit.js --reporter spec"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "package-name": "^0.1.0",
    "proxy-from-env": "0.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^9.11.1",
    "istanbul": "^0.4.2",
    "lolex": "^1.5.0",
    "mocha": "^10.7.3",
    "nock": "^13.5.5",
    "supertest": "^7.0.0"
  },
 "engines": {
  "node": "^18.16.0 || ^20.9.0 || >=21.1.0"
}
}
